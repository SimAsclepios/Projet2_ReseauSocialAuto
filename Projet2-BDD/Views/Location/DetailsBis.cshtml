@model Projet2_BDD.Models.BienMateriel@using Projet2_BDD.Models<section>    <h1>@Html.DisplayFor(model => model.Titre)</h1>    <hr />    <header>        <dl class="dl-horizontal">            <dt>                @Html.DisplayNameFor(model => model.Utilisateur.Prenom)            </dt>            <dd>                @Html.DisplayFor(model => model.Utilisateur.Prenom)            </dd>            <dd>                <img src="~/uploads/@Model.ImageName" width="250" height="250" asp-append-version="true" />            </dd>        </dl>    </header>    <p>        @Html.DisplayFor(model => model.Description)    </p></section><footer>    @Html.ActionLink("Edit", "ModifierBienMateriel", new { id = Model.Id })    @Html.ActionLink("Supprimer", "SupprimerBienMateriel", new { id = Model.Id })    @Html.ActionLink("Retour à la liste", "AfficherBienMateriel")</footer>