@model Projet2_BDD.Models.DemandeServiceImmateriel@using Projet2_BDD.Models<body>    <h1>@Html.DisplayFor(model => model.Titre)</h1>    <hr />    <header>        <dl class="dl-horizontal">            <dt>                @Html.DisplayNameFor(model => model.Prenom)            </dt>            <dd>                @Html.DisplayFor(model => model.Prenom)            </dd>            <dd>                <img src="~/uploads/@Model.ImageName" width="250" height="250" asp-append-version="true" />            </dd>        </dl>    </header>    <article>        @Html.DisplayFor(model => model.Description)    </article>    <div>        <a asp-action="ConformeDmd" asp-route-id="@Model.Id">Autoriser la publication</a>    </div>    <div>        <a asp-action="NonConformeDmd" asp-route-id="@Model.Id">Refuser la publication</a>    </div>    @*        <div>            <a asp-action="EnvoyerMsg" asp-route-utilisateurId="@Model.Id">Contacter</a>        </div>    *@    <footer>        @Html.ActionLink("Contacter", "ListeMessage", "Messagerie", new { Model.UtilisateurId })        @Html.ActionLink("Retour à la liste", "Publication")    </footer></body>