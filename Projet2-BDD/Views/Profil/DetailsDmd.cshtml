@model Projet2_BDD.Models.DemandeServiceImmateriel@using Projet2_BDD.Models<body>    <h1>@Html.DisplayFor(model => model.Titre)</h1>    <hr />    <header>        <dl class="dl-horizontal">            <dt>                @Html.DisplayNameFor(model => model.Prenom)            </dt>            <dd>                @Html.DisplayFor(model => model.Prenom)            </dd>            <dd>                <img src="~/uploads/@Model.ImageName" width="250" height="250" asp-append-version="true" />            </dd>        </dl>    </header>    <article>        @Html.DisplayFor(model => model.Description)    </article>    <footer>        @Html.ActionLink("Modifier la Publication", "ModifierDmd", new { id = Model.Id }) |        @Html.ActionLink("Supprimer la Publication", "SupprimerEvent", new { id = Model.Id }) |        @Html.ActionLink("Retour à la liste", "AfficherPost")    </footer></body>