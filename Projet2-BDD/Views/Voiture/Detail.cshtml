@model Projet2_BDD.Models.Voiture@using Projet2_BDD.Models<section>    <h1>@Html.DisplayFor(model => model.Marque + model.Modele)</h1>    <hr />    <header>        <dl class="dl-horizontal">            <dt>                @Html.DisplayNameFor(model => model.Utilisateur.Prenom)            </dt>            <dd>                @Html.DisplayFor(model => model.Utilisateur.Prenom)            </dd>            <dd>                <img src="~/uploads/@Model.ImageName" width="250" height="250" asp-append-version="true" />            </dd>        </dl>    </header>    <p>        @Html.DisplayFor(model => model.Energie)        @Html.DisplayFor(model => model.Boite)        @Html.DisplayFor(model => model.Km)        @Html.DisplayFor(model => model.Annee)        @Html.DisplayFor(model => model.MontantJour)        @Html.DisplayFor(model => model.MontantKm)        @Html.DisplayFor(model => model.Description)    </p></section><footer>    @Html.ActionLink("Edit", "ModifierVoiture", new { id = Model.Id })    @Html.ActionLink("Supprimer", "SupprimerVoiture", new { id = Model.Id })    @Html.ActionLink("Retour à la liste", "Voiture")</footer>