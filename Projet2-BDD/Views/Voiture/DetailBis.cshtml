@model Projet2_BDD.Models.Covoiturage@using Projet2_BDD.Models<section>    <h1>Détail du covoiturage</h1>    <hr />    <header>        <dl class="dl-horizontal">            <dt>                @Html.DisplayNameFor(model => model.NomVoiture)            </dt>            <dd>                @Html.DisplayFor(model => model.NomVoiture)            </dd>        </dl>    </header>    <p>        @Html.DisplayFor(model => model.VilleEtape)        @Html.DisplayFor(model => model.NbPlace)        @Html.DisplayFor(model => model.Prix)        @Html.DisplayFor(model => model.Description)    </p></section><footer>    @Html.ActionLink("Edit", "ModifierCovoiturage", new { id = Model.Id })    @Html.ActionLink("Supprimer", "SupprimerCovoiturage", new { id = Model.Id })    @Html.ActionLink("Retour à la liste", "Covoiturage")</footer>